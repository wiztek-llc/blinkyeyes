{
  "$schema": "https://raw.githubusercontent.com/nicedoc/tauri/v2/core/tauri-permission-schema/schema.json",
  "identifier": "default",
  "description": "Default capabilities for Blinky",
  "windows": ["main", "overlay"],
  "permissions": [
    "core:default",
    "core:window:allow-show",
    "core:window:allow-hide",
    "core:window:allow-close",
    "core:window:allow-set-focus",
    "core:window:allow-center",
    "core:event:default",
    "notification:default",
    "autostart:default",
    "shell:default"
  ]
}
